{
  "expo": {
    "name": "CRTC",
    "slug": "crtc",
    "scheme": "crtc",
    "version": "3.4.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/906d171b-9101-4954-b4a2-9ef1fb445007"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "th.co.crtc",
      "googleServicesFile": "./GoogleService-Info.plist",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "This app requires access to the camera.",
        "NSLocationWhenInUseUsageDescription": "",
        "NSMicrophoneUsageDescription": "Not use but required.",
        "NSPhotoLibraryUsageDescription": "This app requires access to the photo library.",
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you."
      }
    },
    "android": {
      "package": "th.co.crtc",
      "googleServicesFile": "./google-services.json",
      "versionCode": 49,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "SYSTEM_ALERT_WINDOW",
        "CAMERA",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "DOWNLOAD_WITHOUT_NOTIFICATION",
        "MANAGE_DOCUMENTS"
      ]
    },
    "plugins": [
      "expo-updates"
    ],
    "extra": {
      "eas": {
        "projectId": "906d171b-9101-4954-b4a2-9ef1fb445007"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}